#compdef penv

_penv() {
  local state

  _arguments \
    '1: :->environment'\
    '2: :->variables'

  case $state in
    (environment) _arguments '1:profiles:(staging-1 staging-2 staging-crm staging-sanpenvox automated-tests testapp testing-template)' ;;
    (variables) compadd "$@" RECORDS_REDIS_CACHES_URLS PORT TILES_DB_USER INSIDEVIEW_CLIENT_ID PUBNUB_SUBSCRIBE_KEY DEFAULT_TESTING_DESTINATION_EMAIL MO_TILE_SERVER_URL RECORDS_REDIS_CACHES_PASSWORDS CDN_BASE_URL INSIDEVIEW_API_LOGIN_URL GDAL_LIBRARY_PATH HERE_COM_SECRET_API_KEY APP_SUBDOMAIN LANG DJANGO_SECRET_KEY PGBOUNCER_POOL_MODE REDISCLOUD_URL STORE_ITUNES_RECEIPT CRM_ZOHO_APP_TOKEN SEGMENT_FRONTEND_WRITE_KEY PGBOUNCER_DEFAULT_POOL_SIZE PWD CLOUD_ELEMENTS_COMPANY_SETUP_USER_SECRET RECAPTCHA_KEY CRM_ZOHO_APP_SECRET STATIC_URL CACHE_REDIS_CACHES_URLS CELERY_TIMEZONE CRM_DYNAMICS_APP_TOKEN TILE_SERVER_URL AWS_ACCESS_KEY_ID DEBUG_COLLECTSTATIC MIXPANEL_ID LIBRATO_USER TILES_DB_NAME PROJ4_LIBRARY_PATH CRM_SF_APP_SECRET TRACK_CHECKIN_DELETE_INFORMATION MAPNIK_INPUT_PLUGINS_DIRECTORY TRIAL_EMAIL_FROM_ADDRESS SAMPLE_DATA_USER_ID CUSTOMER_IO_SITE_ID DATABASE_URL TILES_DB_PW PAPERTRAIL_API_TOKEN WEB_CONCURRENCY CACHE_BUSTER_VALUE MEMORY_LOGGING_ENABLED ROUTES_REDIS_CACHES_PASSWORDS CLOUD_ELEMENTS_EVENT_POLLER_REFRESH_INTERVAL CRM_DYNAMICS_APP_SECRET CLOUD_ELEMENTS_ORGANIZATION_SECRET LIBRATO_TOKEN ROUTES_REDIS_CACHES_URLS MAINTENANCE_PAGE_URL GDAL_DATA INSIDEVIEW_API_BASE_URL AWS_STORAGE_BUCKET_NAME GEOS_LIBRARY_PATH ANDROID_MINIMUM_SUPPORTED_VERSION CLOUD_ELEMENTS_USER_SECRET TUTORIAL_MODE_USERS_RATE IOS_MINIMUM_SUPPORTED_VERSION IS_REVIEW_APP PLACES_KEY LIBRATO_PASSWORD HEROKU_API_KEY CRM_SF_APP_TOKEN CACHE_REDIS_CACHES_PASSWORDS POSTMARK_API_KEY CRM_CACHE_TIMEOUT_SECONDS PYTHONPATH CODESHIP_ID TILES_DB_HOST PYTHONUNBUFFERED AWS_SECRET_ACCESS_KEY STRIPE_SECRET_KEY PUBNUB_PUBLISH_KEY CLOUD_ELEMENTS_BASE_URL CRM_IMPORT_PAGE_SIZE C_FORCE_ROOT STRIPE_PUBLISHABLE_KEY CLEAN_VIRTUALENV ENABLE_TESTING_EMAIL_SENDING TILES_DB_PORT PGBOUNCER_LOG_CONNECTIONS PGBOUNCER_LOG_DISCONNECTIONS INSIDEVIEW_CLIENT_SECRET CUSTOMER_IO_API_KEY SEGMENT_WRITE_KEY CRM_GOOGLE_GEOCODE_PRIVATE_KEY DISABLE_COLLECTSTATIC DJANGO_SETTINGS_MODULE CELERY_BROKER_URL
  esac
}

_penv "$@"
